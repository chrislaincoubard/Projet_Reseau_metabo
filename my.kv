#kv du MyApp.py

<LoadDialog>:
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserListView:
            id: filechooser

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: root.cancel()

            Button:
                text: "Load"
                on_release: root.load(filechooser.path, filechooser.selection)

<MyPanel>:
    
    do_default_tab : False
    
    TabbedPanelItem :
    
        text : "Main"
        font_size :  20
        FloatLayout:

            Button:
                id : btn
                text:"Choisissez un \n fichier "
                pos_hint: {"center_x" : 0.5,"center_y" : 0.8}
                on_press : root.call()
            Button:
                id : btn
                text:"Test"
                pos_hint: {"center_x" : 0.5,"center_y" : 0.5}
                on_press : root.test()
            Button:
                id : btn
                text:"TestLoad"
                pos_hint: {"center_x" : 0.5,"center_y" : 0.2}
                on_press : root.show_load()





    TabbedPanelItem :
    
        text : "BLASTing"
        font_size :  20

