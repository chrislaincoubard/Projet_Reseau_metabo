            Button:
                id : btn
                text:"Choisissez un \n fichier modele"
                pos_hint: {"center_x" : 0.5,"center_y" : 0.8}
                on_press : root.show_load()
            Button:
                id : btn
                text:"Choisissez un \n fichier fasta"
                pos_hint: {"center_x" : 0.5,"center_y" : 0.5}
                on_press : root.show_load()

            Button:
                id : btn
                text:"Choisissez un \n fichier fasta "
                pos_hint: {"center_x" : 0.5,"center_y" : 0.2}
                on_press : root.show_load()

    TabbedPanelItem :
    
        text : "MPWTing"
